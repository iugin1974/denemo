<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>KeepOnlySelection</action>
        <after>DeleteSelectionLeaveEmpty</after>
        <_label>Delete All Except Selection</_label>
        <_tooltip>Deletes the whole score apart from the current selection. All other movements, staffs and bars are deleted.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
